                                    <option value="" disabled selected style="display: none;">Faculty</option>
                                    {{#each facultyModel as |facultyChoice|}}
                                        <option value={{facultyChoice.id}}>{{facultyChoice.name}} </option>
                                    {{/each}}
                                </select>
                            </div>
                            <br>
                            <div class="inline field">
                                <br/><br/>
                                <a {{action 'addDeptOption'}}> 
                                    <i class="large green plus square icon"></i>
                                    <br/>
                                </a>
                            </div>
                        </div>

                        <!--DOES NOT SAVE WHEN UPDATING THE FACULTY... DO WE NOT WANT THEM  HAVE THAT OPTION???-->

                        {{#each departmentModel as |deptChoice indexDept|}}
                            <div class="inline fields" id="HS">
                                <div class="inline field">
                                    <br/>
                                    <label style="text-align: center">Option {{indexDept}}: </label>
                                </div>
                                <div class="required inline field">
                                    <label>Name</label>
                                    <input value={{deptChoice.name}} type="text"  onchange = {{action 'changeDeptName' indexHS}} class = "name{{indexDept}}" >
                                </div>
                                <div class="required inline field">
                                    <label>Subject</label>
                                    <select onchange={{action 'changeDeptFaculty' value="target.value"}}>
                                        {{#each facultyModel as |facultyChoice|}}
                                            <option value={{facultyChoice.id}} selected={{eq facultyChoice.id
                                                                                                    deptChoice.faculty.id}}>{{facultyChoice.name}} </option>
                                        {{/each}}
                                    </select>
                                </div>                                
                                <div class="inline field">
                                    <br/>
                                    <a {{action 'removeDeptOption' index}}> 
                                        <i class="large red minus circle icon"></i> <br>
                                    </a>
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
        </div>
    </div>
